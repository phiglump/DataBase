        <Button x:Name="EmployeeButton" Content="See Employee" HorizontalAlignment="Left" Margin="20,256,0,0" VerticalAlignment="Top" Width="375" Height="35" Click="EmployeeButton_Click"/>
        <TextBlock x:Name="TextLabelE" HorizontalAlignment="Left" Margin="20,126,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="109" Width="375" TextDecorations="Underline"/>

    </Grid>
